<Window x:Class="MandelbrotSet.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MandelbrotSet"
        xmlns:VM="clr-namespace:MandelbrotSet"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="940">
    <Window.DataContext>
        <VM:MandelbrotViewModel />
    </Window.DataContext>
    <Grid>
        <local:MandelbrotSetView Grid.Column="0" x:Name="MandelbrotSetView" Map="{Binding Map}">
        </local:MandelbrotSetView>
    </Grid>
</Window>
